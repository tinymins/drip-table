{
  "name": "drip-table-driver-dripd",
  "version": "1.0.1",
  "description": "A drip-table driver based on Drip Design.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "father-build",
    "prepare": "yarn build",
    "postpublish": "git push --tags",
    "lint": "yarn run eslint && yarn run tslint",
    "lint:fix": "yarn run eslint:fix",
    "tslint": "sh ../../bin/tslint.sh **/*.ts",
    "tslint:commit": "sh ./bin/tslint.sh",
    "tslint:exec": "tsc --project .tsconfig-lint.json --skipLibCheck --noEmit",
    "eslint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --format pretty",
    "eslint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --format pretty --fix",
    "eslint:commit": "eslint --format pretty"
  },
  "dependencies": {
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@drip/drip-design": ">=4.9.4",
    "@drip/drip-design-icons": "^4.3.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "drip-table": "link:../drip-table",
    "eslint-config-lvmcn": "^0.0.46",
    "father-build-universal": "^1.21.3",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@drip/drip-design": ">=4.9.4",
    "@drip/drip-design-icons": "^4.3.0",
    "drip-table": "*",
    "react": ">=16.9.0"
  },
  "keywords": [
    "DripTable",
    "Render",
    "TableRender",
    "Drip Design",
    "Json Schema",
    "React"
  ],
  "files": [
    "*.md",
    "dist"
  ],
  "homepage": "http://drip-table.jd.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JDFED/drip-table/issues"
  }
}
